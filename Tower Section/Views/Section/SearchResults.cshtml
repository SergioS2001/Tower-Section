@model List<SectionViewModel>

<h2>Search Results</h2>
@if (Model.Count() < 1)
{
    <p>No matching Sections found.</p>
}
else
{
    <table>
        <tr>
            <th>Part Number</th>
            <th>Bottom Diameter</th>
            <th>Top Diameter</th>
            <!-- Add more table headers for other Section properties -->
        </tr>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.part_number</td>
                <td>@i.bottom_diameter</td>
                <td>@i.top_diameter</td>
                <!-- Add more table data cells for other Section properties -->
            </tr>
        }
    </table>
}

